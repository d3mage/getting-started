    "@inquirer/prompts": "^3.2.0",
    "@kadena/client": "^1.4.0",
    "@kadena/cryptography-utils": "^0.3.8",
    "commander": "^11.0.0",
    "hardhat": "^2.18.1"
  },
  "devDependencies": {
    "@kadena/pactjs-cli": "^1.6.1",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@types/node": "^20.6.3",
    "dotenv": "^16.3.1",
    "ts-node": "^10.9.1"
  }
}
