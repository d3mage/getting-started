{
  "name": "getting-started",
  "version": "1.0.0",
  "description": "5 minute quick-start with Kadena",
  "main": "index.js",
  "scripts": {
    "start": "ts-node -T src/index.ts",
    "deploy": "npm run start -- deploy --keys \"3ebc08c22ec58cb71d05b5cc1ecfa1d54bce03deed7562d7f3a6f9fe890a2b22\" --predicate \"keys-all\" --file src/pact/test-contract.pact --sign-manually",
    "interact": "ts-node src/interact.ts",
    "generate": "npx pactjs contract-generate --file src/pact/test-contract.pact",
    "node": "npx hardhat node",
    "deploy-sol": "npx hardhat run ./deploy-sol.ts --network sepolia"
  },
